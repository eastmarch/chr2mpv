{
  "name": "chr2mpv",
  "version": "1.3",
  "manifest_version": 3,
  "description": "Tries to play the current URL on MPV. Right-click on links or press Ctrl+Shift+Space to activate.",
  "key": "",
  "permissions": ["activeTab", "contextMenus", "scripting"],
  "icons": {
    "128": "img/icon_128.png",
    "64": "img/icon_64.png",
    "32": "img/icon_32.png",
    "16": "img/icon_16.png"
  },
  "action": {
    "default_title": "Play the current URL in MPV"
  },
  "background": {
    "service_worker": "worker.js"
  },
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+Space"
      }
    }
  }
}
